-- Tab Section
local headerHeight = isMobile and 50 or 70
local tabSection = Instance.new("Frame")
tabSection.Size = UDim2.new(0,isMobile and 110 or 200,1,-headerHeight)
tabSection.Position = UDim2.new(0,0,0,headerHeight)
tabSection.BackgroundColor3 = Colors.panel
tabSection.BackgroundTransparency = 0.7
tabSection.BorderSizePixel = 0
tabSection.ZIndex = 201
tabSection.Parent = panel

local tabContainer = Instance.new("ScrollingFrame")
tabContainer.Size = UDim2.new(1,-8,1,-16)
tabContainer.Position = UDim2.new(0,4,0,8)
tabContainer.BackgroundTransparency = 1
tabContainer.BorderSizePixel = 0
tabContainer.ScrollBarThickness = 3
tabContainer.ScrollBarImageColor3 = Colors.accent
tabContainer.ScrollBarImageTransparency = 0.5
tabContainer.ScrollingDirection = Enum.ScrollingDirection.Y -- ONLY VERTICAL
tabContainer.CanvasSize = UDim2.new(0,0,0,0)
tabContainer.ElasticBehavior = Enum.ElasticBehavior.Never
tabContainer.ZIndex = 201
tabContainer.Parent = tabSection

-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
-- ANTI HORIZONTAL & VERTICAL OVER-SCROLL FOR TABS
-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
tabContainer:GetPropertyChangedSignal("CanvasPosition"):Connect(function()
    -- BLOCK HORIZONTAL SCROLL
    if tabContainer.CanvasPosition.X ~= 0 then
        tabContainer.CanvasPosition = Vector2.new(0, tabContainer.CanvasPosition.Y)
    end
    
    -- BLOCK VERTICAL OVER-SCROLL
    local maxScroll = tabContainer.CanvasSize.Y.Offset - tabContainer.AbsoluteSize.Y
    
    if maxScroll <= 0 then
        tabContainer.CanvasPosition = Vector2.new(0, 0)
        return
    end
    
    if tabContainer.CanvasPosition.Y < 0 then
        tabContainer.CanvasPosition = Vector2.new(0, 0)
    elseif tabContainer.CanvasPosition.Y > maxScroll then
        tabContainer.CanvasPosition = Vector2.new(0, maxScroll)
    end
end)

local tabLayout = Instance.new("UIListLayout")
tabLayout.Padding = UDim.new(0,isMobile and 6 or 8)
tabLayout.SortOrder = Enum.SortOrder.LayoutOrder
tabLayout.Parent = tabContainer
tabLayout:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
    tabContainer.CanvasSize = UDim2.new(0,0,0,tabLayout.AbsoluteContentSize.Y+20)
end)

-- Content Section
local tabWidth = isMobile and 110 or 200
local contentSection = Instance.new("Frame")
contentSection.Size = UDim2.new(1,-(tabWidth+8),1,-(headerHeight+8))
contentSection.Position = UDim2.new(0,tabWidth+4,0,headerHeight+4)
contentSection.BackgroundTransparency = 1
contentSection.BorderSizePixel = 0
contentSection.ZIndex = 201
contentSection.ClipsDescendants = true
contentSection.Parent = panel

local contentScroll = Instance.new("ScrollingFrame")
contentScroll.Name = "ContentScroll"
contentScroll.Size = UDim2.new(1,-8,1,-8)
contentScroll.Position = UDim2.new(0,4,0,4)
contentScroll.BackgroundTransparency = 1
contentScroll.BorderSizePixel = 0
contentScroll.ScrollBarThickness = isMobile and 4 or 6
contentScroll.ScrollBarImageColor3 = Colors.accent
contentScroll.ScrollBarImageTransparency = 0.5
contentScroll.ScrollingDirection = Enum.ScrollingDirection.Y -- ONLY VERTICAL
contentScroll.CanvasSize = UDim2.new(0,0,0,0)
contentScroll.ClipsDescendants = true
contentScroll.AutomaticCanvasSize = Enum.AutomaticSize.Y
contentScroll.ElasticBehavior = Enum.ElasticBehavior.Never
contentScroll.ZIndex = 201
contentScroll.Parent = contentSection

-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
-- ANTI HORIZONTAL & VERTICAL OVER-SCROLL FOR CONTENT
-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
contentScroll:GetPropertyChangedSignal("CanvasPosition"):Connect(function()
    -- BLOCK HORIZONTAL SCROLL
    if contentScroll.CanvasPosition.X ~= 0 then
        contentScroll.CanvasPosition = Vector2.new(0, contentScroll.CanvasPosition.Y)
    end
    
    -- BLOCK VERTICAL OVER-SCROLL
    local maxScroll = contentScroll.CanvasSize.Y.Offset - contentScroll.AbsoluteSize.Y
    
    if maxScroll <= 0 then
        contentScroll.CanvasPosition = Vector2.new(0, 0)
        return
    end
    
    if contentScroll.CanvasPosition.Y < 0 then
        contentScroll.CanvasPosition = Vector2.new(0, 0)
    elseif contentScroll.CanvasPosition.Y > maxScroll then
        contentScroll.CanvasPosition = Vector2.new(0, maxScroll)
    end
end)

_G.AdvancedSettings.contentScroll = contentScroll

local contentPadding = Instance.new("UIPadding")
contentPadding.PaddingLeft = UDim.new(0,4)
contentPadding.PaddingRight = UDim.new(0,4)
contentPadding.PaddingTop = UDim.new(0,4)
contentPadding.PaddingBottom = UDim.new(0,4)
contentPadding.Parent = contentScroll

local contentLayout = Instance.new("UIListLayout")
contentLayout.Padding = UDim.new(0,isMobile and 8 or 12)
contentLayout.SortOrder = Enum.SortOrder.LayoutOrder
contentLayout.Parent = contentScroll
contentLayout:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
    contentScroll.CanvasSize = UDim2.new(0,0,0,contentLayout.AbsoluteContentSize.Y+20)
end)

-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
-- TAB SYSTEM
-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

function _G.AdvancedSettings.registerTab(id, name, order, createContentFunc, setupFunc, cleanupFunc)
    table.insert(_G.AdvancedSettings.tabs, {
        id = id,
        name = name,
        order = order,
        createContent = createContentFunc,
        setup = setupFunc,
        cleanup = cleanupFunc
    })
    
    -- Sort by order
    table.sort(_G.AdvancedSettings.tabs, function(a, b)
        return a.order < b.order
    end)
    
    print("âœ… Tab registered: " .. name)
end

local function createTabButton(data)
    local tabBtn = Instance.new("TextButton")
    tabBtn.Name = data.id
    tabBtn.Size = UDim2.new(1,-10,0,isMobile and 35 or 45)
    tabBtn.BackgroundColor3 = Colors.card
    tabBtn.BackgroundTransparency = 1
    tabBtn.BorderSizePixel = 0
    tabBtn.Text = ""
    tabBtn.AutoButtonColor = false
    tabBtn.ZIndex = 202
    tabBtn.LayoutOrder = data.order
    tabBtn.Parent = tabContainer
    Utils.corner(tabBtn, 8)
    
    local indicator = Instance.new("Frame")
    indicator.Size = UDim2.new(0,3,1,-10)
    indicator.Position = UDim2.new(0,0,0,5)
    indicator.BackgroundColor3 = Colors.accent
    indicator.BorderSizePixel = 0
    indicator.BackgroundTransparency = 1
    indicator.ZIndex = 203
    indicator.Parent = tabBtn
    Utils.corner(indicator, 2)
    
    local label = Instance.new("TextLabel")
    label.Size = UDim2.new(1,-15,1,0)
    label.Position = UDim2.new(0,isMobile and 8 or 15,0,0)
    label.BackgroundTransparency = 1
    label.Text = data.name
    label.TextColor3 = Colors.textGray
    label.Font = Enum.Font.GothamBold
    label.TextSize = isMobile and 10 or 14
    label.TextXAlignment = Enum.TextXAlignment.Left
    label.ZIndex = 204
    label.Parent = tabBtn
    
    _G.AdvancedSettings.tabButtons[data.id] = {
        btn = tabBtn,
        indicator = indicator,
        label = label
    }
    
    tabBtn.MouseButton1Click:Connect(function()
        _G.AdvancedSettings.activeTab = data.id
        updateTabUI()
        _G.AdvancedSettings.updateContent()
    end)
    
    tabBtn.MouseEnter:Connect(function()
        if _G.AdvancedSettings.activeTab ~= data.id then
            Utils.tween(tabBtn, TweenInfo.new(0.15), {BackgroundTransparency = 0.7})
        end
    end)
    
    tabBtn.MouseLeave:Connect(function()
        if _G.AdvancedSettings.activeTab ~= data.id then
            Utils.tween(tabBtn, TweenInfo.new(0.15), {BackgroundTransparency = 1})
        end
    end)
    
    return tabBtn
end

function updateTabUI()
    for id, components in pairs(_G.AdvancedSettings.tabButtons) do
        local isActive = (id == _G.AdvancedSettings.activeTab)
        Utils.tween(components.btn, TweenInfo.new(0.25), {BackgroundTransparency = isActive and 0.6 or 1})
        Utils.tween(components.indicator, TweenInfo.new(0.25), {BackgroundTransparency = isActive and 0 or 1})
        Utils.tween(components.label, TweenInfo.new(0.25), {TextColor3 = isActive and Colors.text or Colors.textGray})
    end
end

function _G.AdvancedSettings.updateContent()
    -- Clear content
    for _, child in ipairs(contentScroll:GetChildren()) do
        if child:IsA("Frame") then
            child:Destroy()
        end
    end
    
    -- Find active tab
    local activeTabData = nil
    for _, tab in ipairs(_G.AdvancedSettings.tabs) do
        if tab.id == _G.AdvancedSettings.activeTab then
            activeTabData = tab
            break
        end
    end
    
    -- Create content
    if activeTabData and activeTabData.createContent then
        activeTabData.createContent(contentScroll, Colors, isMobile, Utils)
    end
end

-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
-- MENU TOGGLE
-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

function _G.AdvancedSettings.toggleMenu()
    _G.AdvancedSettings.menuOpen = not _G.AdvancedSettings.menuOpen
    
    if _G.AdvancedSettings.menuOpen then
        Utils.tween(btn, TweenInfo.new(0.2), {BackgroundTransparency = 1})
        Utils.tween(btnStroke, TweenInfo.new(0.2), {Transparency = 1})
        Utils.tween(icon, TweenInfo.new(0.2), {TextTransparency = 1})
        task.delay(0.2, function() btn.Visible = false end)
        
        backdrop.Visible = true
        panel.Visible = true
        Utils.tween(backdrop, TweenInfo.new(0.35), {BackgroundTransparency = 0.25})
        
        -- Start from center with size 0
        panel.Size = UDim2.new(0,0,0,0)
        panel.Position = UDim2.new(0.5,0,0.5,0)
        
        -- Target size based on device
        local targetSize = isMobile and UDim2.new(0.85,0,0.7,0) or UDim2.new(0,900,0,600)
        
        -- Animate to target size (position stays centered due to AnchorPoint)
        Utils.tween(panel, TweenInfo.new(0.35, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {
            Size = targetSize
        })
        
        updateTabUI()
        _G.AdvancedSettings.updateContent()
    else
        Utils.tween(backdrop, TweenInfo.new(0.25), {BackgroundTransparency = 1})
        
        -- Animate to size 0 (position stays centered)
        Utils.tween(panel, TweenInfo.new(0.25, Enum.EasingStyle.Quart, Enum.EasingDirection.In), {
            Size = UDim2.new(0,0,0,0)
        })
        
        task.delay(0.25, function()
            backdrop.Visible = false
            panel.Visible = false
            btn.Visible = true
            btn.BackgroundTransparency = 1
            btnStroke.Transparency = 1
            icon.TextTransparency = 1
            Utils.tween(btn, TweenInfo.new(0.2), {BackgroundTransparency = 0.15})
            Utils.tween(btnStroke, TweenInfo.new(0.2), {Transparency = 0.4})
            Utils.tween(icon, TweenInfo.new(0.2), {TextTransparency = 0})
        end)
    end
end

-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
-- EVENT CONNECTIONS
-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

task.wait(0.2)

pcall(function()
    btn.MouseButton1Click:Connect(_G.AdvancedSettings.toggleMenu)
end)

pcall(function()
    closeBtn.MouseButton1Click:Connect(_G.AdvancedSettings.toggleMenu)
end)

pcall(function()
    backdrop.MouseButton1Click:Connect(_G.AdvancedSettings.toggleMenu)
end)

closeBtn.MouseEnter:Connect(function()
    if closeBtn.Parent then
        Utils.tween(closeBtn, TweenInfo.new(0.15), {BackgroundTransparency = 0.3})
    end
end)

closeBtn.MouseLeave:Connect(function()
    if closeBtn.Parent then
        Utils.tween(closeBtn, TweenInfo.new(0.15), {BackgroundTransparency = 0.5})
    end
end)

-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
-- LOAD TABS
-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

-- Wait for tab modules to load
task.wait(0.5)

-- Create tab buttons after all tabs registered
for _, tab in ipairs(_G.AdvancedSettings.tabs) do
    createTabButton(tab)
    
    -- Run setup if provided
    if tab.setup then
        task.spawn(tab.setup)
    end
end

-- Initial UI update
updateTabUI()
